<UserControl x:Class="WarnMe_Cherry.Steuerelemente.Sites.Settings.dmarItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WarnMe_Cherry.Steuerelemente.Sites.Settings"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Viewbox>
            <TextBlock x:Name="dmarId" Text="[Id]"/>
        </Viewbox>
        <Viewbox Grid.Row="1">
            <TextBlock x:Name="dmarName" Text="[Name]"/>
        </Viewbox>
        <Viewbox Grid.Column="1">
            <TextBlock x:Name="dmarPercent" Text="[%]"/>
        </Viewbox>
    </Grid>
</UserControl>
